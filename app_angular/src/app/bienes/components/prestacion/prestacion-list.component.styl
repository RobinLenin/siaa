.example-container {
    height: 635px;
    overflow: auto;
}
table {
    width: 100%;
}

@media (min-width: 1450px) {
    .example-container {
        height: 635px;
        overflow: auto;
    }
}

@media (max-width: 1400px) {
    .example-container {
        height: 295px;
        overflow: auto;
    }
}

